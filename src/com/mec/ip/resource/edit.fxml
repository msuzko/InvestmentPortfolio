<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" minHeight="-Infinity" minWidth="450.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mec.ip.controllers.EditDialogController">
    <children>
        <HBox minHeight="20.0" prefWidth="200.0">
            <children>
                <Label minWidth="92.0" text="%date">
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </Label>
                <Label minWidth="92.0" text="%ticker">
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </Label>
                <Label minWidth="93.0" text="%count">
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </Label>
                <Label minWidth="93.0" text="%price">
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </Label>
            </children>
        </HBox>
        <HBox prefWidth="200.0">
            <children>
                <TextField fx:id="txtDate" minWidth="92.0" promptText="%date">
                    <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </TextField>
                <TextField fx:id="txtTicker" minWidth="92.0" promptText="%ticker">
                    <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </TextField>
                <TextField fx:id="txtCount" minWidth="93.0" promptText="%count">
                    <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </TextField>
                <TextField fx:id="txtCost" minWidth="93.0" promptText="%price">
                    <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
                    </HBox.margin>
                </TextField>
            </children>
        </HBox>
        <AnchorPane>
            <children>
                <Button fx:id="btnOk" defaultButton="true" layoutX="358.0" layoutY="5.0" minWidth="70.0"
                        mnemonicParsing="false" onAction="#actionSave" prefHeight="27.0" text="%ok"
                        AnchorPane.rightAnchor="85.0" AnchorPane.topAnchor="5.0"/>
                <Button fx:id="btnCancel" cancelButton="true" layoutX="445.0" layoutY="14.0" minWidth="70.0"
                        mnemonicParsing="false" onAction="#actionClose" text="%cancel" AnchorPane.rightAnchor="10.0"
                        AnchorPane.topAnchor="5.0"/>
            </children>
            <VBox.margin>
                <Insets bottom="10.0"/>
            </VBox.margin>
        </AnchorPane>
    </children>
</VBox>
